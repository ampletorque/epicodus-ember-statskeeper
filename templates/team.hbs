<h1>{{name}}</h1>

<h3>Players:</h3>

<ul>
  {{#each player in players}}
    <p>{{#link-to 'player' player.id}}{{player.name}}{{/link-to}}</p>
  {{/each}}
</ul>

<h3>Score:</h3>

{{score}}

<h3>High Scorer:</h3>

<p>{{#link-to 'player' highScorer.id}} {{highScorer.name}} {{/link-to}}</p>

<p>{{#link-to 'new-player' this class="btn btn-default"}}Add a new player{{/link-to}}</p>
{{outlet}}
